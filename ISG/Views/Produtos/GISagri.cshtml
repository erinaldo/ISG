<!-- CONTATO -->
<span class="anchor" id="contact"></span>
<div class="page color-4">
    <div class="container">
        <div class="inner-page">
            <h2 class="page-headline">Entre em contato</h2>
            <!--<div class="color-bottom-line center">
                <h2 class="line center"><span><i class="main-color"></i>Entre em contato</span></h2>
            </div>-->
        </div>
        <div class="row-fluid inner-page contact">
            <div class="span6">
                @using (Ajax.BeginForm("Enviar", null, new AjaxOptions { UpdateTargetId = "msgResponseContato", OnSuccess = "onSuccess", OnBegin = "onBegin", OnComplete = "onComplete" }, new { @id = "contact-form" }))
                {
                    <div id="msgResponseContato" style="background-color: rgba(12, 129, 236, 0.8); height: 30px; width: 100%; display: none; text-align: center"></div>

                    <textarea rows="10" placeholder="Mensagem" name="mensagem"></textarea>

                    <input type="text" placeholder="contato@e-mail.com" name="contato"><br />

                    <input type="text" placeholder="Nome/Empresa" name="nome"><br />
					
                    <input type="submit" class="btn btn-primary btn-centered" value="Contate-nos" />
                }

                <!--<form id="contact-form">
                        <div id="msgResponseContato" style="background-color:red"></div>
					    <textarea rows="6" placeholder="Mensagem"></textarea>
					    <input type="text" placeholder="contato@e-mail.com"><br />
					    <input type="text" placeholder="Nome/Empresa"><br />
					    <button class="btn btn-primary btn-centered">Contate-nos</button>
				    </form>-->
            </div>
            <div class="span6">
                <!-- MAP DIV // !Don't remove this !Important -->
                <!-- MAP DIV // !Don't remove this !Important -->

                <div id="map" style="height:350px"></div>
                <!-- MAP DIV // !Don't remove this !Important -->
            </div>
        </div>
    </div>
</div>
<!-- /CONTATO -->


<script type="text/javascript">
    var onSuccess = function (result) {
        if (result) { $("#msgResponseContato").show("slow"); }
    }

    var onBegin = function () {
        $("#msgResponseContato").hide("slow");
        $("#contact input")[2].value = "Enviando";
        $("#contact input")[2].disabled = true;
    }

    var onComplete = function () {
        $("#contact input")[2].disabled = false;
        $("#contact input")[2].value = "Contate-nos";
    }
    </script>
